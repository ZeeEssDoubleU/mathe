		"@types/react": "^17.0.1",
		"@types/react-dom": "^17.0.0",
		"@types/react-helmet": "^6.1.0",
		"@types/sanitize-html": "^1.27.1",
		"@types/styled-components": "^5.1.7",
		"@types/webpack": "^4.41.26",
		"@typescript-eslint/eslint-plugin": "^4.15.2",
		"@typescript-eslint/parser": "^4.15.2",
		"concurrently": "^6.0.0",
		"dotenv": "^8.2.0",
