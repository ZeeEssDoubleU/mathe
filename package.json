{
	"name": "mathe",
	"private": true,
	"description": "A website showcasing premium tea blends and yerba mate.",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"build": "gatsby build",
		"dev": "concurrently 'yarn start -o' 'yarn test' 'yarn generate --watch'",
		"dev-m": "concurrently 'yarn start -o -H $HOSTNAME -p 8000' 'yarn test' 'yarn generate --watch'",
		"dev-codegen": "yarn get-schema && yarn dev",
		"format": "prettier --write src/**/*.{js, jsx, ts, tsx}",
		"start": "gatsby develop",
		"serve": "gatsby serve -o",
		"serve-m": "gatsby serve -o -H $HOSTNAME -p 9000",
		"test": "yarn type-check",
		"type-check": "tsc --watch",
		"clean": "gatsby clean && yarn dev",
		"clean-build": "gatsby clean && gatsby build && gatsby serve",
		"deep-clean": "gatsby clean && rm -R node_modules && yarn && yarn dev",
		"generate": "graphql-codegen --config codegen.yml",
		"preload-fonts": "gatsby-preload-fonts",
		"get-schema": "node -r esm src/store/shopifySlice/graphql/schema/getSchema.js",
		"get-assets": "node -r esm src/utils/downloadAssets.js"
	},
	"dependencies": {
		"@loadable/component": "^5.14.1",
		"@reduxjs/toolkit": "^1.5.0",
		"@svgr/webpack": "^5.5.0",
		"axios": "^0.21.1",
		"babel-plugin-styled-components": "^1.12.0",
		"body-parser": "^1.19.0",
		"datocms-client": "^3.3.0",
		"express": "^4.17.1",
		"gatsby": "^3.0.4",
		"gatsby-image": "^3.0.0",
		"gatsby-plugin-google-tagmanager": "^3.3.0",
		"gatsby-plugin-image": "^1.0.1",
		"gatsby-plugin-loadable-components-ssr": "^2.1.0",
		"gatsby-plugin-manifest": "^3.0.0",
		"gatsby-plugin-offline": "^4.0.0",
		"gatsby-plugin-react-helmet": "^4.0.0",
		"gatsby-plugin-robots-txt": "^1.5.5",
		"gatsby-plugin-sharp": "^3.0.1",
		"gatsby-plugin-sitemap": "^3.0.0",
		"gatsby-plugin-styled-components": "^4.2.0",
		"gatsby-plugin-svgr": "^3.0.0-beta.0",
		"gatsby-plugin-webfonts": "^2.1.0",
		"gatsby-react-router-scroll": "^4.0.1",
		"gatsby-source-datocms": "^2.5.7",
		"gatsby-source-filesystem": "^3.0.0",
		"gatsby-source-shopify": "^4.0.0",
		"gatsby-transformer-json": "^3.0.0",
		"gatsby-transformer-sharp": "^3.0.0",
		"graphql-request": "^3.4.0",
		"gsap": "^3.5.1",
		"intersection-observer": "^0.12.0",
		"lodash": "^4.17.21",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-helmet": "^6.1.0",
		"react-icons": "^4.1.0",
		"react-inlinesvg": "^2.2.2",
		"react-query": "3.13.3",
